{
    "name": "home_server_ui",
    "version": "1.0.0",
    "private": true,
    "description": "frontend for home server",
    "main": "main.tsx",
    "lint-staged": {
        "src/**/*.{ts,tsx}": [
            "prettier --write",
            "eslint \"src/**/*.{ts,tsx}\""
        ]
    },
    "scripts": {
        "start": "vite --host",
        "dev": "vite --host",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "prepare": "husky install",
        "lint": "eslint \"src/**/*.{ts,tsx}\"",
        "check": "tsc",
        "pre-commit": "lint-staged"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ivnvMkhl/home_server_ui.git"
    },
