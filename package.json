        "eslint-plugin-n": "15.6.0",
        "eslint-plugin-postcss-modules": "2.0.0",
        "eslint-plugin-promise": "6.1.1",
        "eslint-plugin-react": "7.31.11",
        "husky": "8.0.3",
        "lint-staged": "13.1.0",
        "prettier": "2.8.2",
