    "bugs": {
        "url": "https://github.com/ivnvMkhl/home_server_ui/issues"
    },
    "homepage": "https://github.com/ivnvMkhl/home_server_ui#readme",
    "dependencies": {
        "@tanstack/react-query": "4.22.0",
        "antd": "5.1.4",
        "classnames": "2.3.2",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "uuid": "9.0.0"
    },
    "devDependencies": {
        "@types/node": "18.11.18",
        "@types/react": "18.0.26",
        "@types/react-dom": "18.0.10",
        "@types/uuid": "9.0.0",
